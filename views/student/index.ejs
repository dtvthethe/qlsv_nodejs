<h1>Danh sách sinh viên</h1>
    <a href="add.html" class="btn btn-info">Add</a>
    <form action="list.html" method="GET">
      <label class="form-inline justify-content-end">Tìm kiếm: <input type="search" name="search" class="form-control"
          value="">
        <button class="btn btn-danger">Tìm</button>
      </label>
    </form>
    <table class="table table-hover">
      <thead>
        <tr>
          <th>#</th>
          <th>Mã SV</th>
          <th>Tên</th>
          <th>Ngày Sinh</th>
          <th>Giới Tính</th>
          <th></th>
          <th></th>
        </tr>
      </thead>
      <tbody>
        <% rows.forEach((row, index) => { %>
        <tr>
          <td><%=index+1%></td>
          <td><%=row.id%></td>
          <td><%=row.name%></td>
          <td><%=row.birthday%></td>
          <td><%=row.gender%></td>
          <td><a href="edit.html">Sửa</a></td>
          <td><a data="1" class="delete" href="list.html" type="student">Xóa</a></td>
        </tr>
        <%})%>
      </tbody>
    </table>
    <div>
      <span>Số lượng: 3</span>
    </div>
